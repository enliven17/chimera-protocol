# Environment Variables
# Copy this file to .env and fill in your values

# Private key of your wallet (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Ethereum Sepolia RPC URL (Infura, Alchemy, etc.)
ETHEREUM_SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_PROJECT_ID

# Etherscan API key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key

# Hedera testnet account ID (optional)
HEDERA_ACCOUNT_ID=0.0.123456

# PYUSD token addresses
PYUSD_ETHEREUM_SEPOLIA=0x... # PYUSD token address on Ethereum Sepolia
PYUSD_HEDERA_TESTNET=0x...   # PYUSD OFT Adapter address on Hedera Testnet

# LayerZero endpoint addresses
LAYERZERO_HEDERA_ENDPOINT=0x6EDCE65403992e310A62460808c4b910D972f10f
LAYERZERO_ETHEREUM_SEPOLIA_ENDPOINT=0x6EDCE65403992e310A62460808c4b910D972f10f
